{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": ["Chrome >= 37", "ff >= 45", "Safari > 8", "IE >= 10"]
        },
        "loose": true
      }
    ]
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]]
}
